{
  "name": "hanford-internet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate && yarn generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "prisma generate",
    "generate": "graphql-codegen",
    "dev": "concurrently \"yarn next dev\" \"yarn generate --watch './graphql/**/*.ts'\"",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\""
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@boost/module": "^3.0.1",
    "@giraphql/core": "^2.16.0",
    "@giraphql/plugin-prisma": "^0.10.0",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "@mantine/core": "^3.0.5",
    "@mantine/hooks": "^3.0.5",
    "@mantine/rte": "^3.0.5",
    "@prisma/client": "^3.3.0",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "graphql": "^15.7.1",
    "graphql-helix": "^1.8.4",
    "graphql-scalars": "^1.12.0",
    "next": "12.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rpio": "^2.4.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/schema-ast": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-react-apollo": "^3.1.6",
    "@types/faker": "^5.5.9",
    "@types/react": "17.0.33",
    "@types/rpio": "^2.4.2",
    "autoprefixer": "^10.3.7",
    "concurrently": "^6.3.0",
    "eslint": "8.1.0",
    "eslint-config-next": "12.0.1",
    "faker": "^5.5.3",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "prisma": "^3.3.0",
    "tailwindcss": "^2.2.17",
    "ts-node": "^10.4.0",
    "typescript": "4.4.4"
  }
}
